class Main {
    public static void main(String[] args) {
        int[] arr={17,3,5,7,9,11,18};
        int LengthOfArray=arr.length;
        int MaxNumber=arr[0];
        for(int i=0;i<LengthOfArray;i++){
            if (arr[i]>MaxNumber)
            MaxNumber=arr[i];
        }
        System.out.println("The Maximum Number Of The Array Is "+MaxNumber);
    }
}
